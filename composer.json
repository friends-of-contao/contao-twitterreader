{
    "name": "lindesbs/twitterreader",
    "description": "TwitterReader for the Contao Open Source CMS",
    "keywords": ["twitter", "contao"],
    "type": "contao-module",
    "homepage": "http://www.lindesbs.de",
    "license": [
        "LGPL-3.0+",
        "CC-BY-NC-3.0"
    ],
    "authors": [
        {
            "name": "Stefan Lindecke",
            "homepage": "http://www.lindesbs.de",
            "role": "Developer"
        },
        {
            "name": "Kirsten Roschanski",
            "homepage": "http://kirsten-roschanski.de/",
            "email": "kontakt@kirsten-roschanski.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/lindesbs/twitterreader/issues",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/lindesbs/twitterreader",
        "forum": "https://community.contao.org/de/forumdisplay.php?119-Sonstige-Erweiterungen"
    },
    "require": {
        "php": ">=5.3",
        "contao/core": ">=2.11,<4",
		"contao-community-alliance/composer-plugin":"~2.0"
    },
    "autoload": {
        "classmap":["src/"]
    },
    "replace": {
        "contao-legacy/twitterreader": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "src": "system/modules/twitterreader"
            },
            "runonce":[
              "src/config/runonce.php"
            ]
        },
	"branch-alias": {
		"dev-master": "1.4.x-dev"
	}
    }
}
